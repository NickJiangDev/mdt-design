  "description": "MDT Design | React组件库",
  "source": "src/index.tsx",
  "main": "cjs/index.js",
  "umd": "umd/index.js",
  "module": "esm/index.js",
  "browser": "bundle.js",
  "files": [
    "cjs/*",
    "esm/*",
    "umd/*",
    "bundle.js",
    "bundle.css"
  ],
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "gc": "node scripts/generate-component.js",
    "start": "start-storybook -p 6006 --no-manager-cache",
    "bs": "build-storybook -o docs",
    "gulp": "gulp",
    "esm": "gulp watch-esm",
    "cjs": "gulp watch-cjs",
    "dev": "gulp watch",
    "build:esm": "gulp esm",
    "build:cjs": "gulp cjs",
    "build": "run-p gulp build:umd build:min build:browser",
    "build:browser": "rollup -c --environment FORMAT:iife",
    "build:umd": "rollup -c --environment FORMAT:umd",
    "build:min": "rollup -c --environment FORMAT:min",
    "gen-pkg": "cat ./package.json | grep -v '\"private\":' | grep -v '\"postinstall\":' > dist/package.json",
    "cp_readme": "cp README.md dist/README.md",
    "test": "craco test",
    "test-ci": "CI=true craco test --passWithNoTests --coverage",
    "prettier": "prettier -c --write 'src/**/*'",
    "prettier:stage": "prettier -c --write",
    "tsc:check": "rimraf .tsc && tsc  && rimraf .tsc",
    "lint": "run-s lint:ts lint:style lint:prettier",
    "lint:ts": "yarn tsc:check && eslint --fix --cache --ext .ts,.tsx src/",
    "lint-stage:ts": "yarn tsc:check && eslint --fix",
    "lint:style": "stylelint --fix 'src/**/*.less' --syntax less",
    "lint-stage:style": "stylelint --fix --syntax less",
    "stylelint-check": "stylelint-config-prettier-check",
    "lint:prettier": "prettier --check --write \"**/*.{tsx,ts,less,md,json}\" --end-of-line auto",
    "mvdist": "rimraf dist && mkdir dist && mv esm dist && mv cjs dist && mv umd dist && mv bundle.js dist && mv bundle.css dist",
    "prepub": "run-s lint test-ci build mvdist rv gen-pkg cp_readme",
    "prepub-ci": "run-s build mvdist rv gen-pkg cp_readme",
    "install-peers": "npm-install-peers",
    "ct": "git add .",
    "cz": "git cz",
    "rv": "HUSKY=0 standard-version",
    "rv:minor": "HUSKY=0 standard-version --release-as minor",
    "rv:patch": "HUSKY=0 standard-version --release-as patch",
    "rv:major": "HUSKY=0 standard-version --release-as major",
    "gitlab-release-init": "conventional-gitlab-releaser-fork -p angular -r 0 --url https://gitlab.idatatlas.com/api/v4/ -c $PWD/.conventional-changelog-writer.js",
    "release2dingtalk": "conventional-dingtalk -p angular -l Mdt-design版本更新 -x 'Mdt-design 前端版本更新'",
    "gitlab-release": "conventional-gitlab-releaser-fork -p angular --url https://gitlab.idatatlas.com/api/v4/ -c $PWD/.conventional-changelog-writer.js"
  },
